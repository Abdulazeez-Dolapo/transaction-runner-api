{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node  ./src/app.js",
    "lint": "eslint src/  --fix",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel ./src --out-dir ./build",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js",
    "test": "set NODE_ENV=test&&sequelize db:migrate:undo:all&&sequelize db:migrate&&nyc --require @babel/register  mocha ./src/tests/** --timeout 10000 --exit",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov | coveralls",
    "migration": "npm run migrate:undo && npm run migrate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.13.14",
    "@babel/runtime": "^7.12.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "faker": "^5.5.3",
    "helmet": "^3.22.0",
    "http-errors": "~1.6.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1",
    "shortid": "^2.2.15",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^5.1.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.1",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^9.0.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.9",
    "nyc": "^15.1.0"
  }
}
